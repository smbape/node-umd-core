{
  "name": "umd-core",
  "version": "3.0.1",
  "authors": [
    "St√©phane MBAPE"
  ],
  "description": "umd-core files",
  "homepage": "https://github.com/smbape/node-umd-core",
  "main": [
    "lib/**",
    "src/**"
  ],
  "umd": true,
  "license": "MIT",
  "ignore": [
    "**/.*",
    "excluded",
    "bower_components",
    "node_modules",
    "test",
    "tests"
  ],
  "peerDependencies": {
    "backbone" : "git+https://github.com/jashkenas/backbone#^1.3.3",
    "i18next" : "git+https://github.com/i18next/i18next#^19.7.0",
    "jquery" : "git+https://github.com/jquery/jquery-dist#^3.3.1",
    "lodash" : "git+https://github.com/lodash/lodash#^4.17.11",
    "qs" : "git+https://github.com/hapijs/qs#^6.6.0",
    "react": "https://unpkg.com/react@16/umd/react.production.min.js",
    "react-dom": "https://unpkg.com/react-dom@16/umd/react-dom.production.min.js"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "jquery": "*",
        "lodash": "*"
      }
    }
  }
}
